{
  "name": "@tillerxr/use-stripe-subscription-next",
  "license": "MIT",
  "homepage": "https://github.com/tillerxr/use-stripe-subscription-next",
  "repository": {
    "type": "git",
    "url": "https://github.com/tillerxr/use-stripe-subscription-next"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/tillerxr",
    "access": "public"
  },
  "version": "1.2.6",
  "scripts": {
    "rollup-build-lib": "rollup -c"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/esm/client/index.js",
      "require": "./dist/cjs/client/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/esm/server/index.js",
      "require": "./dist/cjs/server/index.js"
    }
  },
  "files": [
    "dist",
    "client",
    "server"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/react": "^18.0.28",
    "rollup": "^3.29.0",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-multi-input": "^1.4.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@stripe/stripe-js": "^1.31.0",
    "stripe": "^9.6.0",
    "swr": "^1.3.0"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
